{
    "name" : "Brave Fix",
    "version":"1.0",
    "manifest_version":2,
    "icons": {
        "128": "icon_128.png",
        "48": "icon_48.png",
        "16": "icon_16.png"
    },
    "content_scripts":[
        {
            "matches":["<all_urls>"],
            "js" : ["content.js"]
        }
    ],
    "permissions": [
        "tabs"
    ],
    "browser_action":{
        "default_title":"Brave Fix"
    }
}